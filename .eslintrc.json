{
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "extends": [
    "standard",
    "standard-jsx"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "strict": 0
  }
}